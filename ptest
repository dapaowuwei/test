# -*- coding: utf-8 -*-

import math

print('请依次输入a,b,c的值：')
#a=float(input())
#b=float(input())
#c=float(input())

def quadratic(a, b, c):
    if b*b-4*a*c>0:
        x1=(-b+math.sqrt(b*b-4*a*c))/(2*a)
        x2=(-b-math.sqrt(b*b-4*a*c))/(2*a)
        print('存在两个解：')
        print('x1={0},x2={1}'.format(x1,x2))
    elif b*b-4*a*c==0:
        x=-b/(2*a)
        print('存在唯一解：')
        print(x)
    else:
        print('无解')
